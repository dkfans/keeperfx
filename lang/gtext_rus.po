msgctxt "Level name"
msgid "Cosyton"
msgstr "Козитон"

#: guitext:204
msgctxt "Level name"
msgid "Waterdream Warm"
msgstr "Уотердрим Ворм"

#: guitext:205
msgctxt "Level name"
msgid "Flowerhat"
msgstr "Флауэрхат"

#: guitext:206
msgctxt "Level name"
msgid "Lushmeadow-on-Down"
msgstr "Лашмедоу-он-Даун"

#: guitext:207
msgctxt "Level name"
msgid "Snuggledell"
msgstr "Снагглдейл"

#: guitext:208
msgctxt "Level name"
msgid "Wishvale"
msgstr "Вишвейл"

#: guitext:209
msgctxt "Level name"
msgid "Tickle"
msgstr "Тикл"

#: guitext:210
msgctxt "Level name"
msgid "Moonbrush Wood"
msgstr "Мунбраш-Вуд"

#: guitext:211
msgctxt "Level name"
msgid "Nevergrim"
msgstr "Невергрим"

#: guitext:212
msgctxt "Level name"
msgid "Hearth"
msgstr "Харт"

#: guitext:213
msgctxt "Level name"
msgid "Elf's Dance"
msgstr "Элфс-Дэнс"

